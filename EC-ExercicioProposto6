package estrutura_condicional;

import java.util.Locale;
import java.util.Scanner;

public class exercicioProposto6 {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
        //define qual local o texto se baseara
		Scanner sc = new Scanner(System.in);
		
		double X = sc.nextDouble();
		
		if (X < 0.0 || X > 100.0) {
        //if = se
        //se o resultado de x for menor que 0 ou maior que 100 o resultado abaixo sera impresso 
			System.out.println("Fora de intervalo");
		}
		else if (X <= 25.0) {
        //else if = ou se
        //se o resultado de x ficar entre 0 e 25 o resultado abaixo sera impresso 
			System.out.println("Intervalo [0,25]");
		}
		else if (X <= 50.0) {
        //else if = ou se
        //se o resultado de x ficar entre 25 e 50 o resultado abaixo sera impresso 
			System.out.println("Intervalo [25,50]");
		}
		else if (X <= 75.0) {
        //else if = ou se
        //se o resultado de x ficar entre 50 e 75 o resultado abaixo sera impresso 
			System.out.println("Intervalo [50,75]");
		}
		else {
        //else = ou
        //se o resultado de x ficar entre 75 e 100 o resultado abaixo sera impresso 
			System.out.println("Intervalo [75,100]");
		}
		sc.close();
	}

}
